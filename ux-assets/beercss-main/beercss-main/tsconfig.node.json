{
  "compilerOptions": {
    "composite": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitAny": true,
    "jsx": "preserve"
  },
  "vueCompilerOptions": {
    "target": 3,
    "extensions": [
      ".vue"
    ],
    "plugins": ["@vue/language-plugin-pug"]
  },
}
